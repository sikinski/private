@media only screen and (max-width 1680px)
    .start
        height calc(640px + (812 - 640) * ((100vw - 320px) / (1680 - 320)))
        .start-main
            .heading
                font-size calc(18px + (72 - 18) * ((100vw - 320px) / (1680 - 320)))
                