@media only screen and (max-width 425px)
    .logo-wrapper
        width 17%
        .logo-company 
            width 57.25px
            height 25px
        .logo-text
            visibility hidden
    .start
        padding 12px 15px
        .start-main
            .desc 
                max-width 100%
            .search
                width 112px
                form
                    .name-or-TIN-input
                        width 93%
                    .search-company-btn
                        height 40%
                        font-size 9px
    .key-capabilities
        padding 50px 20px
        .grid-wrapper 
            grid-auto-rows minmax(100px, auto)
            grid-gap 10px
    .indicators
        .text-wrapper
            .view-data-btn
                width 80%
    .vedprofile-API
        .content
            .data-companies
                .learn-more-btn
                    width 80%
    .tariffs
        .tariff-container
            .tariff
                .arrange-btn
                    width 80%
    .numbers
        .number-block
            .text
                margin 20px 0
    .demo-access
        .block
            .inputs
                .get-demo-access
                    width 80%

