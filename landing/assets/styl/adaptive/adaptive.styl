@media only screen and (max-width 1500px)
    .indicators
        .pics-wrapper
            .pics
                width 75%
        .text-wrapper
            .view-data-btn
                margin-top 80.6px
    .key-capabilities
        .grid-wrapper 
            grid-template-columns 34.6% 34.1% 31.1% //532px 573.9px 574px 


@media only screen and (max-width 1367px)
    .start
        padding 27.5px 65px 
        .header-btns
              .language-btn
                display flex
                margin 0 54px
                align-items: center

                .flag-country
                  width 25px
                  height 25px
                  margin-right 5px

                .name-language
                  font-size 12px

              .sign-in-btn
                width 130px
                height 30px
        .start-main
            .heading
                max-width 100%
            .search
                height 50px
                form
                    .name-or-TIN-input
                        width 490px
                    .name-or-TIN-input::placeholder
                        font-weight 600
                        font-size 14px
                    .search-company-btn
                        width 214px
                        font-size: 12px
    .logo-company 
        width 60px
        height 26px
    .logo-text
        font-size 30px
        margin-left 7px

    .fea
        font-size 19.7px
        margin-left 6px

    .advantages
        align-items: center
        padding-bottom 66px
        .advs-wrapper
            width 37.8% //586px
            .adv-container
                padding-right 15px
            .icon
                width 37px
                height 37px
                margin auto 20px
    .indicators
        padding 70px 
        .pics-wrapper
            .pics
                width 65%
        .text-wrapper
            .view-data-btn
                width 303px
                height 55px
                margin-top 50.6px
    .vedprofile-API
        .content
            margin-left 10%
            .pic-wrapper
                .pic
                    position relative
    .demo-access
        padding 63px 87px 80px 87px

    .numbers
        .number-block
            flex-direction column
            // text-align center
            // justify-content flex-start
            align-items flex-start
        .block-1, .block-2, .block-3
            justify-content flex-start
        .block-4, .block-5, .block-6
            justify-content flex-end

    .tariffs
        padding 170px 110px 137.19px 130px

@media only screen and (max-width 1230px)
    .advantages
        .advs-wrapper
            width 38%
            .adv-container
                padding 18px 18px 18px 0
                height auto
                

    .key-capabilities
        .grid-wrapper 
            .phone
                margin-top 60px
    .green-line-desc
        font-size: 14px
    .indicators
        padding 92px 50px 72px 65px
        .text-wrapper
            .overhead-text
                font-size 11px
            .title
                font-size: 35px
                line-height 45px
                margin-bottom 50px

            .view-data-btn
                width 263px
                height 49px
                font-size 13px
    .vedprofile-API
        .content
            .data-companies
                .learn-more-btn
                    width 263px
                    height 49px
                    font-size 12px


@media only screen and (max-width 1100px)
    .key-capabilities
        .grid-wrapper 
            .item
                .text
                    margin-top 12px
            .item-1, .item-3, .item-5
                .text
                    margin-top 82px
    .vedprofile-API
        .content
            .circles-icon
                display none
    .tariffs
        padding 100px 80px 
        .desc 
            font-size: 18px
            line-height 32px
            
    .demo-access
        .block
            .text-wrapper
                padding 93px 0 93px 41px
            .inputs
                padding 93px 0 
                input
                    font-size: 16px
                input::placeholder
                    font-size 14px
            .get-demo-access
                width 263px
                height 49px
                font-size 12px
    .logo-company 
        width 52px
        height 22px
    .logo-text
        font-size 28px
        margin-left 7px
    .fea
        font-size 18px
        margin-left 6px
    .footer
        padding 10px 36px

